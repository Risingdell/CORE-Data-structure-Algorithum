/**
 * @param {number[]} prices
 * @return {number}
 */
var maxProfit = function(prices) {
    //find minimum
    let min=prices[0];
    let max=0;
    let result=0
    let p=0;
    for(let i=0;i<prices.length;i++){
        min=Math.min(prices[i],min);
        p=prices[i]-min;
        max=Math.max(p,max);
    }
    return max;
   

    
    
};

console.log(maxProfit([7,1,5,3,6,4]));